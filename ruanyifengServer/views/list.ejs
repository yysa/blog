<!-- 列表页 -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>列表页</title>
    <link rel="stylesheet" href="<%=webstatic%>css/reset.css">
    <link rel="stylesheet" href="<%=webstatic%>css/clipboard.css">
</head>
<body>
    <div class="body">
        <div class="header clearfix">
            <h1 class="fl">阮一峰的网络日志<span> » <a href="<%=host%>">首页</a> » <a href="#">档案</a></span></h1>
            <div class="seach fr clearfix">
                <div class="icon fl">
                    <a href="#">
                        <img src="data:image/png;base64," alt="">
                    </a>
                </div>
                <input class="fr" type="text" name="" id="">
                <div class="seach-btn"></div>
            </div>
        </div>
        <div class="content clearfix">
            <div class="content-b">
                <p class="content-b-title">
                        <a>分类</a>：<%=category[0].cname%>（共<span><%=allAct.length%></span>篇文章）
                </p>
                <% for(var key in groupAct){%>
                    <div class="newpages">
                    <h3><%=key%>年</h3>
                    <ul>
                        <%groupAct[key].forEach(function(item){%>
                            <li><a href="<%=host+'blog/article/'+item.id%>"><%=item.title%></a><i>（<span><%=item.count%>@<%=timeChange(item.create_time,3)%></span>）</i></li>
                        <%})%>
                    </ul>
                    </div>
                <%}%>
                
            </div>
            <div class="content-a">
                <div class="">
                    <h2>分类</h2>
                    <ul>
                        <% allCategory.forEach(function(item){%>
                            <li><a href="<%=host+'blog/list/'+item.id%>"><%=item.cname%></a>（<span><%=item.count%></span>）</li>                            
                        <%})%>
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer">
            <p>2018 @ <a>我的邮件</a>|<a>博客</a>|<a>推特</a>|<a>GitHub</a></p>
        </div>
    </div>
</body>
</html>